{
  "name": "caracakes",
  "version": "1.0.0",
  "description": "Site for cake orders",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.css -w",
    "watch:gall": "node-sass sass/gall.scss css/gall.css -w",
    "watch:user": "node-sass sass/user.scss css/user.css -w",
    "watch:shop": "node-sass sass/shop.scss css/shop.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass watch:gall watch:user watch:shop"
    
  },
  "author": "Jume Brice",
  "license": "ISC",
  "dependencies": {
    "node-sass": "^4.13.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.6.1",
    "npm-run-all": "^4.1.5"
  }
}
